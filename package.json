{
  "name": "turnip-agent",
  "version": "0.1.0",
  "description": "An agent for generating and tokenizing unique AI art on-chain using OpenAI, PumpPortal, and Helius",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "turnip": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "ai",
    "art",
    "openai",
    "nft",
    "token",
    "pumpfun",
    "helius",
    "agent",
    "generative-art"
